@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Qt@QT_VERSION_MAJOR@ COMPONENTS Core DBus)
find_dependency(DtkCore)
include(${CMAKE_CURRENT_LIST_DIR}/Dtk6SystemSettingsTargets.cmake)
set(Dtk6SystemSettings_LIBRARIES Dtk::SystemSettings)
get_target_property(Dtk6SystemSettings_INCLUDE_DIRS Dtk::SystemSettings INTERFACE_INCLUDE_DIRECTORIES)
get_target_property(Dtk6SystemSettings_LIBRARY_DIRS Dtk::SystemSettings INTERFACE_LINK_DIRECTORIES)

set(Dtk6SystemSettings_FOUND true)
